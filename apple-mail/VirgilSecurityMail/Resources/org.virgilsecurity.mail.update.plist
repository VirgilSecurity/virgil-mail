<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnableTransactions</key>
	<false/>
	<key>KeepAlive</key>
	<false/>
	<key>Label</key>
	<string>org.virgilsecurity.mail.update</string>
	<key>MachServices</key>
	<dict>
		<key>org.virgilsecurity.mail.update_Mach</key>
		<true/>
	</dict>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/Mail/Bundles/VirgilSecurityMail.mailbundle/Contents/Resources/VirgilUpdate.app/Contents/MacOS/VirgilUpdate</string>
	</array>
	<key>StartInterval</key>
	<integer>900</integer>
</dict>
</plist>